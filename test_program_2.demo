// Knapsack problem
procedure knapsack
{
    // Input:
    // Values (stored in array v)
    // Weights (stored in array w)
    // Number of distinct items (n)
    // Knapsack capacity (W)

    int i,j;
    int n, W;
    int m[1:10000, 1:10000];
    int v[1:10000], w[1:10000];

    read n;
    read W;
    read v;
    read w;

    for j = 0 to W by 1
    {
        m[0, j] = 0;
    }

    for i = 1 to n by 1
    {
        for j = 0 to W by 1
        {
            if (w[i-1] > j) then
            {
                m[i, j] = m[i-1, j];
            }
            else
            {
                if (m[i-1, j] > m[i-1, j-w[i-1]] + v[i-1]) then
                {
                    m[i, j] = m[i-1, j];
                }
                else
                {
                    m[i, j] = m[i-1, j-w[i-1]] + v[i-1];
                }
            }
        }
    }

    write m;
}