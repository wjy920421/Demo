// test DEMO program
procedure reverse {
    
    int i, n;
    char temp, ch[1:10000]; // array

    read n; // read number

    for i = 1 to n by 1
    {
        read ch[i]; // read array
    }

    for i = 1 to n / 2 by 1 // swap
    {
        temp = ch[i];
        ch[i] = ch[n+1-i];
        ch[n+1-i] = temp;
    }

    for i = 1 to n by 1
    {
        write ch[i]; // print array
    }
}




